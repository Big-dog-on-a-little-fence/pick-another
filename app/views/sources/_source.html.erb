<h3 align="center" class="page-header">Resources</h3>

<table class="table table-striped">
  <thead>
    <tr>
      <th class="col-md-1"></th><!-- empty for edit buttons -->
      <th class="col-md-2">Version</th>
      <th class="col-md-7"></th>
      <th class="col-md-2">Last Update</th>
    </tr>
  </thead>
  <tbody>
    <% obj.each do |source| %>
      <tr>
        <td><%= link_to 'Edit', edit_tune_source_path(@tune, source), class: "btn btn-xs btn-warning" %></td>
        <td><%= source.version %></td>
        <td>
          <%= embed(source.link) %>
          <div><%= source.description %></div>
        </td>
        <td><%= time_ago_in_words(source.updated_at) %> ago</td>
      </tr>
    <% end %>
  </tbody>
</table>
